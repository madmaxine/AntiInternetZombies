{
  "name": "AntiInternetZombie",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension will let you know when you're cycling through websites over and over again!",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/options_custom/background2.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/options_custom/index.html"
  },
  "permissions": [
    "history"
  ]
}